{
  "name": "@beakerx/beakerx-widgets",
  "version": "2.3.11",
  "description": "BeakerX: Beaker EasyForms, Magics, Plots and Spark Extension for Jupyter Lab 2.x",
  "homepage": "http://beakerx.com/",
  "keywords": [
    "beakerx",
    "jupyter",
    "jupyterlab",
    "jupyterlab notebook",
    "jupyterlab-extension",
    "widgets"
  ],
  "author": {
    "name": "Two Sigma Open Source, LLC",
    "email": "beakerx-feedback@twosigma.com"
  },
  "main": "lib/jupyterlab-plugin",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/twosigma/beakerx_widgets.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:css && npm run build:js && webpack",
    "build:css": "npm run build:css:classic && npm run build:css:lab",
    "build:css:classic": "sass --no-source-map --load-path node_modules sass/beakerx_widgets.scss css/beakerx_widgets.css",
    "build:css:lab": "sass --no-source-map --load-path node_modules sass/beakerx_widgets_lab.scss css/beakerx_widgets_lab.css",
    "build:js": "tsc",
    "build:labextension": "npm run build && jupyter labextension build .",
    "build:labextension:dev": "npm run build && jupyter labextension build --development True .",
    "clean": "rimraf lib css dist beakerx/static beakerx/labextension tsconfig.tsbuildinfo",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "prepublish": "npm run build",
    "prettier": "prettier --check src/**/*.{ts,js}",
    "prettier:fix": "prettier --check src/**/*.{ts,js} --write",
    "test": "karma start --single-run",
    "watch": "run-p watch:js watch:webpack",
    "watch:js": "tsc --watch",
    "watch:webpack": "webpack --watch --progress"
  },
  "devDependencies": {
    "@jupyterlab/application": "^4.0.4",
    "@jupyterlab/apputils": "^4.1.4",
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/cells": "^4.0.4",
    "@jupyterlab/codemirror": "^4.0.4",
    "@jupyterlab/coreutils": "^6.0.4",
    "@jupyterlab/docregistry": "^4.0.4",
    "@jupyterlab/notebook": "^4.0.4",
    "@jupyterlab/settingregistry": "^4.0.4",
    "@types/big.js": "^4.0.5",
    "@types/chai": "^4.2.11",
    "@types/d3": "^5.7.2",
    "@types/expect.js": "^0.3.29",
    "@types/jquery": "^3.3.38",
    "@types/jqueryui": "^1.12.10",
    "@types/json-schema": "^7.0.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.24",
    "@types/react": "^18.0.26",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "file-loader": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.65.0",
    "sass-loader": "^13.3.0",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.0",
    "typescript": "~5.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "yjs": "^13.5.40"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "@jupyter-widgets/controls": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "@lumino/commands": "^2.0.1",
    "@lumino/coreutils": "^2.0.0",
    "@lumino/disposable": "^2.0.0",
    "@lumino/messaging": "^2.0.0",
    "@lumino/signaling": "^2.0.0",
    "@lumino/widgets": "^2.0.1",
    "big.js": "^5.2.2",
    "d3": "^5.16.0",
    "flatpickr": "^4.6.3",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui.combobox": "^1.0.7",
    "moment-timezone": "^0.5.28",
    "underscore": "^1.10.2"
  },
  "files": [
    "css/",
    "dist/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff,woff2,ttf}",
    "lib/",
    "fonts/"
  ],
  "jupyterlab": {
    "extension": "lib/jupyterlab-plugin",
    "outputDir": "beakerx/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "none"
        }
      ],
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": false
        }
      ],
      "curly": [
        "error",
        "all"
      ],
      "eqeqeq": "error",
      "prefer-arrow-callback": "error"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
